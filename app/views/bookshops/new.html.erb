<% provide(:title, '店舗登録') %>
<h1>店舗登録</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@bookshop) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :phone %>
      <%= f.text_field :phone %>

      <%= f.label :image_data %>
      <%= f.file_field :image_data %>

      <%= f.label :address %>
      <%= f.text_field :address %>

      <%= f.label :area %>
      <%= f.text_field :area %>

      <%= f.label :station %>
      <%= f.text_field :station %>

      <%= f.label :url %>
      <%= f.text_field :url %>

      <%= f.label :twitter_url %>
      <%= f.text_field :twitter_url %>
      
      <%= f.label :google_map_url %>
      <%= f.text_field :google_map_url %>
      
      <%= f.label :business_hour %>
      <%= f.text_field :business_hour %>
      
      <%= f.label :holiday %>
      <%= f.text_field :holiday %>
            
      <%= f.label :access %>
      <%= f.text_field :access %>
            
      <%= f.label :comments_count %>
      <%= f.text_field :comments_count %>




      <%= f.submit "これで登録", class: "btn btn-primary" %>
    <% end %>
  </div>
</div> 