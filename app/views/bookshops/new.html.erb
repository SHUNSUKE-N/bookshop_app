<% provide(:title, '店舗登録') %>
<h1>店舗登録</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@bookshop) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :image_data %>
      <%= f.text_field :image_data %>

      <%= f.label :address %>
      <%= f.text_field :address %>

      <%= f.label :area %>
      <%= f.text_field :area %>


      <%= f.submit "これで登録", class: "btn btn-primary" %>
    <% end %>
  </div>
</div> 